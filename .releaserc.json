{
    "branch": "master",
    "plugins": [
        [
            "@semantic-release/exec",
            {
                "prepareCmd": "envman add --key NEW_APP_VERSION --value ${nextRelease.version}",
                "publishCmd": "npm version ${nextRelease.version} --git-tag-version=false"
            }
        ],
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/git"
    ],
    "dryRun": false,
    "ci": true
}